{
  "name": "@prognovel/prognovel-cli",
  "version": "0.0.1",
  "description": "A repo to manage contents of ProgNovel platform.",
  "main": ".prognovel/prognovel.js",
  "bin": {
    "@prognovel/prognovel-cli": ".prognovel/bin/index.js",
    "prognovel": ".prognovel/bin/index.js"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:both": "npm run build:types && npm run build",
    "build": "rollup -c",
    "publish": "npm run build && git add . && git commit -m \"update novels...\" && git push",
    "dev": "node .prognovel/.development/server.js",
    "format": "prettier --write .prognovel/**/*.{ts,js,md}",
    "test": "test",
    "start": "node .prognovel/.dist/main.js --enable-source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProgNovel/prognovel-contents.git"
  },
  "author": "ProgNovel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ProgNovel/prognovel-contents/issues"
  },
  "homepage": "https://github.com/ProgNovel/prognovel-contents#readme",
  "dependencies": {
    "alphanum-sort": "^1.0.2",
    "brotli": "^1.3.2",
    "chalk": "^4.1.0",
    "dessert-yaml-js": "^1.0.3",
    "front-matter": "^3.1.0",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "globby": "^11.0.1",
    "js-yaml": "^3.14.0",
    "markdown-wasm": "^1.1.2",
    "marked": "^1.1.1",
    "meow": "^7.0.1",
    "polka": "^0.5.2",
    "sharp": "^0.25.4",
    "string-similarity": "^4.0.3",
    "tiny-glob": "^0.2.6",
    "wasm-frontmatter": "^1.0.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-regenerator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-wasm": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "prettier": "^2.2.1",
    "rollup": "^2.38.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-regenerator": "^0.6.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.3"
  }
}
